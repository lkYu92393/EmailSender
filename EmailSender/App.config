<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
	<appSettings>
		<add key="IsShowTemplate" value="Y" />
		<add key="NumOfCheckLogDate" value="7" />
		<!--<add key="FileExcelPath" value="email" />-->
		<add key="FileExcel" value="templateExcel.xlsx" />
		<add key="ExcelHasHeader" value="Y" />
		<add key="ExcelColumns" value="name|Email|Attachment" />
		<add key="FileTemplateMsg" value="templateMsg.txt" />
		<!--<add key="AttachmentPath" value="attachment" />-->
		<add key="UserAlias" value="alias" />
		<add key="EmailUser" value="loginName" />
		<add key="EmailPW" value="loginPW" />
		<add key="EmailServer" value="smtp.live.com" />
		<add key="EmailPort" value="587" />
	</appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="BouncyCastle.Crypto" publicKeyToken="0e99375e54769942" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.9.0.0" newVersion="1.9.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="MimeKit" publicKeyToken="bede1c8a46c66814" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>